{
  "name": "citytweetstream",
  "version": "1.0.0",
  "description": "A geostream of tweets from major American cities",
  "main": "server.js",
  "scripts": {
    "server": "node server.js",
    "webpack-watch": "webpack -w",
    "dev": "concurrently --kill-others \"npm run webpack-watch\" \"npm run server \"",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amjacobowitz/citytweetstream.git"
  },
  "keywords": [
    "find",
    "locate",
    "locator",
    "finder",
    "twitter",
    "tweet"
  ],
  "author": "Aaron Jacobowitz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amjacobowitz/citytweetstream/issues"
  },
  "homepage": "https://github.com/amjacobowitz/citytweetstream#readme",
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "d3": "^4.2.2",
    "d3-geo": "^1.2.3",
    "express": "^4.14.0",
    "json-loader": "^0.5.4",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-faux-dom": "^2.7.1",
    "react-scrollbar": "^0.4.1",
    "react-socket": "^0.1.4",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "topojson": "^1.6.26",
    "twitter": "^1.4.0",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.2"
  }
